{
  "name": "mw-design",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "csstype": "^2.6.9",
    "react": "^16.12.0",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "start": "styleguidist server",
    "build": "yarn build:style && yarn build:lib",
    "build:style": "rm -rf ./styleguide && styleguidist build",
    "build:lib": "rm -rf ./lib && tsc",
    "clean": "rm -rf ./lib && rm -rf ./styleguide && rm -rf ./node_modules",
    "test": "node_modules/.bin/jest",
    "test:coverage": "yarn test --coverage && open coverage/lcov-report/index.html",
    "test:coveralls": "yarn test --coverage --coverageReporters=text-lcov | coveralls",
    "eject": "react-scripts eject",
    "eslint": "eslint './src/**/*.{ts,tsx}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.3",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.0",
    "@types/styled-system": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript": "^1.16.2",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "react-styleguidist": "^10.6.2",
    "ts-jest": "^25.2.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0"
  }
}
